<p id="notice"><%= notice %></p>

<h1>Add Item</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Price</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @menus.each do |menu| %>
      <tr>
        <td><%= menu.code %></td>
        <td><%= menu.name %></td>
        <td><%= menu.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>

